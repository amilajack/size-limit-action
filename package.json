{
  "name": "size-limit-action",
  "version": "1.4.0",
  "private": true,
  "description": "size-limit action",
  "main": "dist/index.js",
  "license": "ISC",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "build": "ncc build src/main.ts -o dist",
    "size-build": "npm run build",
    "size": "npm run size-build && size-limit"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "4500 ms"
    }
  ],
  "dependencies": {
    "@actions/artifact": "^0.5.0",
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.3",
    "@actions/github": "^4.0.0",
    "@actions/glob": "^0.1.1",
    "bytes": "^3.1.0",
    "github-fetch-workflow-artifact": "^1.0.4",
    "has-yarn": "^2.1.0",
    "markdown-table": "^2.0.0"
  },
  "devDependencies": {
    "@size-limit/file": "^4.9.1",
    "@size-limit/time": "^4.9.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.7.12",
    "@typescript-eslint/parser": "^4.14.0",
    "@zeit/ncc": "^0.22.1",
    "eslint": "^7.18.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^3.13.1",
    "prettier": "^2.2.1",
    "size-limit": "^4.9.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
